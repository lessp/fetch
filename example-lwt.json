    "devDependencies": {
      "@reason-native/console": "0.1.0",
      "@esy-ocaml/reason": ">=3.6.0"
    },

    "resolutions": {
      "fetch-core": "link:./fetch-core.json",
      "fetch-native-lwt": "link:./fetch-native-lwt.json"
    }
  }
}
