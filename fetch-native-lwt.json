{
  "name": "fetch-native-lwt",
  "version": "0.1.0",
  "description": "Fetch client (Lwt) for native ReasonML/OCaml",
  "license": "MIT",
  "esy": {
    "buildEnv": {
      "PKG_CONFIG_PATH": "/usr/lib64/pkgconfig:$PKG_CONFIG_PATH"
    },
    "exportedEnv": {
      "PKG_CONFIG_PATH": {
        "val": "/usr/lib64/pkgconfig:$PKG_CONFIG_PATH",
        "scope": "global"
      }
    },
    "build": "dune build -p #{self.name}",
    "buildDev": "refmterr dune build -p #{self.name}"
  },
  "scripts": {
    "format": "esy dune build @fmt --auto-promote"
  },
  "dependencies": {
    "@opam/dune": "*",
    "@opam/reason": "*",
    "fetch-core": "0.1.0-alpha.4",
    "@reason-native-web/piaf": "1.0.0-alpha.1"
  },
  "devDependencies": {
    "ocaml": "~4.9.0",
    "refmterr": "*"
  },
  "resolutions": {
    "pkg-config": "lessp/pkg-config#16264492e05232f01acd4fabe7a2fe70c7ea5ad8",
    "@opam/conf-libssl": "esy-packages/esy-openssl#648930fa614886bc189f9b5c31f64f0b663fceee"
  }
}