{
  "name": "fetch-native-lwt",
  "version": "0.1.0",
  "description": "Fetch client (Lwt) for native ReasonML/OCaml",
  "license": "MIT",
  "esy": {
    "build": "dune build -p #{self.name}",
    "buildDev": "refmterr dune build -p #{self.name}"
  },
  "scripts": {
    "format": "esy @#{self.name} dune build @fmt --auto-promote",
    "doc": "esy @#{self.name} dune build @doc",
    "doc-path": "esy @#{self.name} echo #{self.target_dir}/default/_doc/_html/index.html"
  },
  "dependencies": {
    "fetch-core": "*",
    "@esy-ocaml/reason": ">=3.3.2",
    "@opam/dune": ">=2.0.1",
    "@reason-native-web/piaf": "1.0.1002",
    "ocaml": ">=4.8.0"
  },
  "devDependencies": {
    "@opam/merlin": "*",
    "@reason-native/rely": "*",
    "refmterr": "*",
    "ocaml": "~4.9.0",
    "@opam/odoc": "*"
  },
  "resolutions": {
    "fetch-core": "link:./fetch-core.json"
  }
}
