{
  "name": "fetch-core",
  "version": "0.1.0-alpha.4",
  "description": "A fetch interface/functor for ReasonML/OCaml",
  "license": "MIT",
  "esy": {
    "build": "dune build -p #{self.name}",
    "buildDev": "refmterr dune build -p #{self.name}"
  },
  "scripts": {
    "format": "esy @#{self.name} dune build @fmt --auto-promote",
    "test": "esy @#{self.name} dune exec FetchTestRunner",
    "test:watch": "esy @#{self.name} dune runtest test --watch --force"
  },
  "dependencies": {
    "@opam/dune": "*",
    "@esy-ocaml/reason": "*",
    "ocaml": ">=4.8.0"
  },
  "devDependencies": {
    "ocaml": "~4.9.0",
    "refmterr": "*",
    "@reason-native/rely": "3.2.1"
  }
}
