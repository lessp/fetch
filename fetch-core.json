{
  "name": "fetch-core",
  "version": "0.1.0",
  "description": "A fetch interface/functor for ReasonML/OCaml",
  "license": "MIT",
  "esy": {
    "build": "dune build --profile=release --only-packages=fetch-core --root .",
    "buildDev": "refmterr dune build @all"
  },
  "scripts": {
    "format": "esy dune build @fmt --auto-promote"
  },
  "dependencies": {
    "@opam/dune": "*"
  },
  "peerDependencies": {
    "@opam/reason": "*",
    "ocaml": "*"
  },
  "devDependencies": {
    "refmterr": "^3.2.2",
    "@opam/merlin": "^3.3.2",
    "@opam/rtop": "3.5.0",
    "@opam/utop": "2.4.2"
  }
}
