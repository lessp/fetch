    "@opam/dune": "*",
    "@opam/reason": "*"
  },
  "peerDependencies": {
    "ocaml": ">=4.6.0"
  },
  "devDependencies": {
    "@opam/merlin": "^3.3.2",
    "@opam/rtop": "*",
    "@opam/utop": "*",
    "ocaml": "<4.9.0",
    "refmterr": "*"
  }
}
